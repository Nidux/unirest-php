{
  "name": "nidux/unirest-php",
  "description": "Unirest PHP",
  "keywords": ["rest", "curl", "http", "https", "client"],
  "type": "library",
  "homepage": "https://github.com/nidux/unirest-php",
  "license": "MIT",
  "require": {
    "php": ">=5.4.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "suggest": {
    "ext-json": "Allows using JSON Bodies for sending and parsing requests"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "codeclimate/php-test-reporter": "0.4.4"
  },
  "autoload": {
    "psr-0": {
      "Unirest\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/Unirest",
      "Unirest\\": "src/"
    }
  }
}
